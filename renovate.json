{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "groupName": "all dependencies",
  "groupSlug": "all",
  "schedule": "* * 1 */1 *",
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true
  },
  "packageRules": [
    {
      "groupName": "all dependencies",
      "groupSlug": "all",
      "matchPackageNames": ["*"]
    }
  ],
  "ignoreDeps": [],
  "ignorePaths": []
}
